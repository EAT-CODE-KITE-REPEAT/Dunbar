{
  "expo": {
    "name": "Dunbar",
    "description": "Personal and Professional Relationship Management (PPRM)",
    "slug": "Dunbar",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0054b8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "3",
      "bundleIdentifier": "com.progenworks.dunbar",
      "icon": "./assets/icon.png"
    },
    "android": {
      "versionCode": 1,
      "package": "com.progenworks.dunbar",
      "permissions": [
        "CAMERA",
        "READ_CONTACTS",
        "com.android.launcher.permission.INSTALL_SHORTCUT",
        "com.htc.launcher.permission.READ_SETTINGS",
        "com.htc.launcher.permission.UPDATE_SHORTCUT",
        "com.majeur.launcher.permission.UPDATE_BADGE"
      ]
    }
  }
}
